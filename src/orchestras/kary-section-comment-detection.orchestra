 {\n    background-color:           #FFFDF7;\n    border-radius:              2pt;\n    color:                      #BC670E;\n    font-family:                \"Source Code Pro\";\n    font-size:                  0.85em;\n    font-variant-ligatures:     contextual !important;\n    font-weight:                400;\n    padding:                    0.5pt 3pt 0.5pt 3pt;\n    vertical-align:             center;\n}\n\nsection {\n    margin-bottom:      2rem;\n}\n\nh1 {\n    color:          black;\n    font-size:      1.4em;\n    margin:         0.2em 0 0.7em 0;\n    padding:        0;\n}\n\n/*\n * ─── ARTICLE ────────────────────────────────────────────────────────────────────\n */\n\n.kf-topic-page-article, p {\n    line-height:    1.6rem !important;\n}\n\n@media ( max-width: 640pt ) {\n    p {\n        font-size:      1.4rem !important;\n    }\n\n    .kf-topic-page-article {\n        font-size:      1.4rem !important;\n    }\n}\n\n.kf-topic-page-spacer {\n    height:     40px;\n}\n\n.kf-topic-page-article h2 {\n    border-top:         1px solid black;\n    font-size:          0.8em;\n    letter-spacing:     10px;\n    margin-bottom:      14px;\n    padding-bottom:     1px;\n    padding-top:        4px;\n    text-align:         right;\n    text-transform:     uppercase;\n}\n\n@media screen and ( max-width: 500px ) {\n    .kf-topic-page-article h2 {\n        font-size:      0.75rem;\n        font-weight:    700;\n    }\n}\n\n.kf-topic-page-article img {\n    border-radius:      3pt;\n    margin-top:         10pt;\n    width:              100%;\n}\n\n.kf-topic-page-article li {\n    font-family:        'Cormorant';\n    font-size:          1em;\n    margin-bottom:      0.7rem;\n}\n\n.kf-topic-page-article b, .kf-topic-page-article strong {\n    font-family:        \"Montserrat\";\n    font-size:          0.8em;\n    font-weight:        bold;\n    text-transform:     capitalize;\n}\n\n@media ( max-width: 26cm ) {\n    .kf-topic-page-article {\n        padding-right:      0, 4rem, 0, 4rem;\n    }\n}\n\n@media ( max-width: 7cm ) {\n    .kf-topic-page-article {\n        margin:     0, 1rem, 0, 1rem;\n    }\n}\n\n/*\n * ─── HEADER ─────────────────────────────────────────────────────────────────────\n */\n\n.kf-topic-page-header {\n    background-color:       #141414;\n    background-position:    center;\n    background-repeat:      no-repeat;\n    background-size:        cover;\n    font-size:              2.6rem;\n    padding-top:            15rem;\n}\n\n@media ( max-width: 550px ) {\n    .kf-topic-page-header {\n        background-position:    left;\n    }\n}\n\n.kf-topic-page-tabs {\n    display:        flex;\n    height:         2.2rem;\n    position:       relative;\n    top:            -2.2rem;\n}\n\n.kf-topic-tab-middle {\n    max-width:              fit-content;\n    background-color:       white;\n    font-size:              1.2rem;\n    font-weight:            700;\n    min-height:             2.2rem;\n    min-width:              fit-content;\n    overflow:               hidden;\n    padding:                0.5em 0 0 0;\n    user-select:            none;\n    -moz-user-select:       none;\n    -webkit-user-select:    none;\n}\n\n.kf-topic-tab-side {\n    background-image:       url('/media/topicpage/tab-left.svg');\n    background-position:    center;\n    background-repeat:      no-repeat;\n    background-size:        cover;\n    flex:                   auto;\n    height:                 2.2rem;\n    max-width:              2.2rem;\n    min-width:              2.2rem;\n    width:                  2.2rem;\n}\n\n.kf-topic-tab-side.left {\n    background-position:    center right;\n}\n\n.kf-topic-tab-side.right {\n    background-position:    center left;\n    margin-left:            -1px;\n    transform:              scaleX(-1);\n}\n\n/*\n * ─── TEXT SECTION ───────────────────────────────────────────────────────────────\n */\n\n.kf-topic-page-main-container {\n    background-color:       white;\n}\n\n.kf-topic-page-section-back {\n    background-image:       url('/media/topicpage/section-back.png');\n    background-repeat:      repeat-x;\n    background-size:        contain;\n    margin-bottom:          1rem;\n    margin-top:             2rem;\n    width:                  100%;\n}\n\n.kf-topic-page-section-text {\n    background-color:       black;\n    font-size:              1.3rem;\n    margin-left:            3rem;\n    padding-left:           0.5rem;\n    padding-right:          0.5rem;\n    width:                  max-content;\n}\n\n.kf-topic-page-main {\n    color:              black;\n    font-size:          1.1rem;\n    line-height:        1.3;\n    margin-left:        auto;\n    margin-right:       auto;\n    max-width:          60vw;\n    padding-bottom:     1.6rem;\n    padding-right:      6rem;\n}\n\n@media ( max-width: 1000px ) {\n    .kf-topic-page-main {\n        margin-left:        3rem;\n        max-width:          100vw;\n        padding-right:      15rem;\n    }\n}\n\n@media ( max-width: 26cm ) {\n    .kf-topic-page-main {\n        margin-left:        2rem;\n        margin-right:       2rem;\n        padding-right:      0;\n    }\n}\n\n/*\n * ─── SIDEBAR ────────────────────────────────────────────────────────────────────\n */\n\n#kf-topic-page-sidebar {\n    -moz-user-select:       none;\n    -ms-user-select:        none;\n    -webkit-user-select:    none;\n    right:                  9vw;\n    user-select:            none;\n    width:                  9rem;\n}\n\n@media ( max-width: 1000px ) {\n    #kf-topic-page-sidebar {\n        right:      7vw;\n    }\n}\n\n@media ( max-width: 26cm ) {\n    #kf-topic-page-sidebar {\n        display:    none;\n    }\n}\n\n#kf-topic-toc {\n    max-height:             calc( 100vh - 150pt );\n    overflow-x:             hidden;\n    overflow-y:             scroll;\n    transition-duration:    0.5s;\n    padding-bottom:         15px;\n}\n\n.toc-on-top #kf-topic-toc {\n    max-height:             calc( 100vh - 250pt );\n    transition-duration:    0.2s;\n}\n\n/*\n * ─── TABLE OF CONTENTS SHADOW ───────────────────────────────────────────────────\n */\n\n.kf-topic-sidebar-overflow-top .kf-topic-toc-bottom-shadow {\n    height:         0;\n    position:       relative;\n    top:            -1px;\n    left:           15pt;\n    right:          0;\n    background:     linear-gradient(transparent, transparent);\n    height:         50px;\n}\n\n.kf-topic-sidebar-overflow-bottom .kf-topic-toc-bottom-shadow, .kf-topic-sidebar-overflow-middle .kf-topic-toc-bottom-shadow {\n    top:            -51px;\n    background:     linear-gradient(transparent, white);\n    height:         50px;\n}\n\n.kf-topic-page-sidebar-box {\n    background-color:       white;\n    border:                 1pt solid #f1f1f1;\n    border-radius:          5pt;\n    box-shadow:             0 0 6pt rgba( 0, 0, 0, 0.15 );\n    padding:                15pt;\n    padding-bottom:         0;\n    transition-duration:    0.5s;\n    width:                  100%;\n}\n\n.kf-topic-page-sidebar-title {\n    font-size:      2rem;\n}\n\n.kf-topic-page-sidebar-box {\n    color:          #555555;\n    font-size:      0.7rem;\n}\n\n#kf-sidebar-title {\n    background-image:       url('/media/topicpage/worldwide.svg');\n    background-position:    left;\n    background-repeat:      no-repeat;\n    background-size:        contain;\n    border-bottom:          1px #aaa solid;\n    font-size:              1.2rem;\n    margin-bottom:          10px;\n    margin-top:             0.3rem;\n    padding-bottom:         1px;\n    padding-left:           2rem;\n}\n\n.kf-topic-page-sidebar-box h2 {\n    color:          #009BFF;\n    font-size:      1rem;\n    padding-top:    0.5rem;\n}\n\n.legend-section {\n    align-items:        flex-start;\n    display:            flex;\n    flex-direction:     row;\n    justify-content:    flex-start;\n}\n\n.legend-paragraph {\n    flex:       3;\n}\n\n@media ( max-width: 800 ) {\n    .legend-paragraph {\n        flex:       2;\n    }\n}\n\n.legend-sidebar {\n    flex-basis:     auto;\n    flex-grow:      1;\n    font-size:      0.8em;\n    font-weight:    400;\n    margin-left:    10pt;\n    max-width:      200px;\n    min-width:      160px;\n}\n\n.legend-sidebar ul {\n    margin-top:     0;\n    padding-top:    0;\n}\n\n.legend-bullet {\n    color:              #848484;\n    display:            flex;\n    font-family:        'Source Code Pro';\n    font-size:          0.8em;\n    font-weight:        500;\n    line-height:        1.5;\n    margin-bottom:      10pt;\n}\n\n.legend-bullet-number {\n    flex:           auto;\n    max-width:      fit-content;\n    padding-top:    0.5pt;\n}\n\n.legend-bullet-content {\n    border-left:    1.5pt #eee solid;\n    flex:           auto;\n    margin-left:    4pt;\n    padding:        0.5pt 0 1.5pt 4pt;\n}\n\n.legend-bullet-content code {\n    font-size:      0.6rem;\n}\n\n.legend-sup a, .legend-sup a:visited {\n    background:     none;\n    color:          black;\n}\n\n.legend-sup a:focus, .legend-sup a:hover {\n    background:         none;\n    text-decoration:    underline;\n}\n\n@media ( max-width: 20cm ) {\n    .legend-sidebar {\n        display:    none;\n        flex:       0;\n    }\n\n    .legend-sup {\n        display:    none;\n    }\n}\n\n.kf-sidebar-H1 {\n    color:              #1C7AB8;\n    font-size:          1rem;\n    margin-bottom:      0.4em;\n    padding:            0.4rem;\n}\n\n.kf-sidebar-H1:hover {\n    background-color:       rgba(224, 238, 250, 0.521);\n    color:                  #1C7AB8 !important;\n}\n\n.kf-sidebar-H1 a, .kf-sidebar-H1 a:visited {\n    color:      inherit;\n}\n\n.kf-sidebar-H2 {\n    border-left:            1.5px #47AC19 solid;\n    clear:                  left;\n    color:                  #424242;\n    display:                block;\n    font-size:              0.7rem;\n    letter-spacing:         2px;\n    margin-left:            1px;\n    max-width:              calc( 100% - 15px );\n    padding:                6px 0 6px 15px;\n    transition-duration:    0.4s;\n}\n\n.toc-on-middle .kf-sidebar-H2 {\n    border-left-color:      transparent;\n    padding-left:           5px;\n    transition-duration:    0.4s;\n}\n\n.kf-sidebar-H2 a, .kf-sidebar-H2 a:visited {\n    color:      inherit;\n}\n\n.kf-sidebar-H1:hover, .kf-sidebar-H2:hover {\n    color:      #3F8027;\n    cursor:     default;\n}\n\n.kf-sidebar-H2:hover {\n    background-color:       rgba(63, 128, 39, 0.1);\n}\n\n.toc-on-top {\n    position:       absolute;\n    top:            13rem;\n}\n\n.toc-on-middle {\n    position:       fixed;\n    top:            80px;\n}\n\n.toc-on-bottom .kf-topic-page-sidebar-box, .toc-on-middle .kf-topic-page-sidebar-box {\n    box-shadow:     0 0 0 transparent;\n}\n\n.kf-topic-banner {\n    float:              left;\n    margin-right:       20px;\n    margin-top:         5px;\n    width:              calc( 50% - 20px );\n}\n\n.kf-topic-banner a, .kf-topic-banner a:hover {\n    background:     none;\n}\n\n#kf-topic-page-header-title {\n    font-size:              1.2rem;\n    border-left:            2px solid #5D5D5D;\n    color:                  white;\n    left:                   12rem;\n    padding-left:           0.9rem;\n    padding-top:            0.2rem;\n    position:               fixed;\n    top:                    0.6rem;\n    transition-duration:    0.3s;\n    user-select:            none;\n    -moz-user-select:       none;\n    -ms-user-select:        none;\n    -webkit-user-select:    none;\n    z-index:                2;\n}\n\n#kf-topic-page-header-title-link {\n    padding-bottom:         0.15em;\n    border-bottom:          0.1em solid transparent;\n    transition-duration:    0.3s;\n}\n\n#kf-topic-page-header-title-link:hover {\n    transition-duration:    0.2s;\n    border-bottom-color:    red;\n}\n\n.kf-topic-page-header-title-hidden {\n    opacity:    0;\n}\n\n.kf-topic-page-header-title-visible {\n    opacity:    100%;\n}\n\n@media ( max-width: 650pt ) {\n    #kf-topic-page-header-title {\n        display:    none !important;\n        opacity:    0;\n    }\n}\n","compilerOptions":{"target":"es6","format":"regexp"}}